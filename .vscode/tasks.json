{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build OpenAPIAndADMDemo",
            "command": "dotnet",
            "type": "process",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Build the project",
            "args": [
                "build",
                "${workspaceFolder}/open_api_adm_dotnet.sln"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Run OpenAPIAndADMDemo",
            "detail": "Runs the application OpenAPIAndADMDemo.exe",
            "command": "OpenAPIAndADMDemo.exe",
            "type": "process",
            "args": [],
            "group": {
                "kind": "test",
                "isDefault": true
            },
            "options": {
                "cwd": "${workspaceFolder}/OpenAPIAndADMDemo/bin/Debug/net48"
            }
        }
    ]
}